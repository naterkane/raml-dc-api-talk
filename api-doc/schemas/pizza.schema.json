{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "plate": {
      "type": "object",
      "properties": {
        "food": {
          "type": "string",
          "default": "pizza",
          "enum": [
            "pizza",
            "fries",
            "veggie burgers",
            "processed cancer death feed"
          ]
        }
      },
      "required": [
        "food"
      ]
    },
    "napkin": {
      "type": "string",
      "enum": [
        "paper",
        "cloth"
      ]
    }
  },
  "required": [
    "plate",
    "napkin"
  ]
}
